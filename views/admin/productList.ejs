        <script>
           
            function changeProductStatus(productId){
                console.log("clicked",productId);
                swal("Are you sure you want to do this?",{
                    buttons:["NO","YES"]
                }).then((res) =>{
                    if(res){
                        $.ajax({
                            url:"/admin/delete-product/"+productId,
                            type:'get'
                        }).done((res)=>{
                            swal("successful",`${res.message}`,"success").then(() =>{
                                location.reload();
                            })
                        })
                        .fail((res) =>{
                            swal("successful",`${res.message}`,"success").then(() =>{
                                location.reload();
                            })
                        })
                    }
                })
            }
        </script>
