    <script>
      const addToCart = (productId) => {

    let size="";
    if(document.getElementById('smallQuantity').classList.contains('active')){
        size="S";
    }
    else if(document.getElementById('mediumQuantity').classList.contains('active')){
        size="M";
    }
    else if(document.getElementById('largeQuantity').classList.contains('active')){
        size="L";
    }else{
        size="M";
    }
        console.log(productId);
        $.ajax({
          url:`/addToCart/${productId}/${size}`,
          method: "POST",
          success: function (response) {
            console.log(response)
            if (response.status) {
              Toastify({
                text: "Added to Cart",
                duration: 3000,
                gravity: "top", // top or bottom
                position: "center", // left, center or right
                stopOnFocus: true, // Prevents dismissing of toast on hover
                style: {
                  background: "green",
                },
              }).showToast();
              setTimeout(() => {
                location.reload();
              }, 600);
            }
            else if(!response.status){
              Toastify({
                text: `${response.message}`,
                duration: 3000,
                gravity: "top", // top or bottom
                position: "center", // left, center or right
                stopOnFocus: true, // Prevents dismissing of toast on hover
                style: {
                  background: "red",
                },
              }).showToast();

            }
            else if(!response.user){

              window.location.href = "/login"
            } else {
              Toastify({
                text: "No stock available",
                duration: 3000,
                gravity: "top", // top or bottom
                position: "center", // left, center or right
                stopOnFocus: true, // Prevents dismissing of toast on hover
                style: {
                  background: "red",
                },
              }).showToast();
            }
          },
          error: function (XHR, textStatus, errorThrown) {
            console.log(errorThrown);
          },
        });
      };
